# This is an example of a property file for configuring OUD
#
# Usage: Not invoked directly
#

IDSTORE_HOST: <LDAP_HOST>
IDSTORE_PORT: <LDAP_PORT>
IDSTORE_SSL_ENABLED: <LDAP_SECURE>
IDSTORE_ADMIN_PORT: <OUD_ADMIN_PORT>

IDSTORE_KEYSTORE_FILE: <OUD_TRUST_STORE>
IDSTORE_KEYSTORE_PASSWORD: <OUD_TRUST_PWD>
IDSTORE_ADMIN_KEYSTORE_FILE: <OUD_ADMIN_TRUST_STORE>
IDSTORE_ADMIN_KEYSTORE_PASSWORD: <OUD_ADMIN_TRUST_PWD>

IDSTORE_BINDDN: <LDAP_ADMIN_USER>
IDSTORE_PASSWD: <LDAP_ADMIN_PWD>
IDSTORE_GROUPSEARCHBASE: <LDAP_GROUP_SEARCHBASE>
IDSTORE_SEARCHBASE: <LDAP_SEARCHBASE>
IDSTORE_USERNAMEATTRIBUTE: cn
IDSTORE_LOGINATTRIBUTE: uid
OAM_SERVER_LOGIN_ATTRIBUTE: uid
IDSTORE_USERSEARCHBASE: <LDAP_USER_SEARCHBASE>
IDSTORE_NEW_SETUP: true
IDSTORE_DIRECTORYTYPE: OUD
# OAM
IDSTORE_OAMADMINUSER:  <LDAP_OAMADMIN_USER>
IDSTORE_OAMSOFTWAREUSER: <LDAP_OAMLDAP_USER>
OAM_IDSTORE_ROLE_SECURITY_ADMIN: <LDAP_OAMADMIN_GRP>
# OAM and OIM
IDSTORE_SYSTEMIDBASE: <LDAP_SYSTEMIDS>
# OIM
IDSTORE_OIMADMINGROUP: <LDAP_OIGADMIN_GRP>
IDSTORE_OIMADMINUSER: <LDAP_OIGLDAP_USER>
# WebLogic
IDSTORE_WLSADMINUSER : <LDAP_WLSADMIN_USER>
IDSTORE_WLSADMINGROUP : <LDAP_WLSADMIN_GRP>

SSL_DEBUG_ENABLE: FALSE


IDSTORE_PWD_OAMSOFTWAREUSER: <LDAP_USER_PWD>
IDSTORE_PWD_OAMADMINUSER: <LDAP_USER_PWD>
OAM_IDM_DOMAIN_WEBGATE_PASSWD:<LDAP_USER_PWD>
IDSTORE_PWD_OIMADMINUSER: <LDAP_USER_PWD>
IDSTORE_PWD_WEBLOGICADMINUSER: <LDAP_USER_PWD>
IDSTORE_PWD_XELSYSADMINUSER: <LDAP_USER_PWD>
IDSTORE_PWD_OAMOBLIXUSER: <LDAP_USER_PWD>

LOG_LEVEL: ALL
